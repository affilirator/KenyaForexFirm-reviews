---
import { fetchRegulatorGlobal } from '~/lib/regulatorGlobals';
import type { AsicRegulationGlobal } from '~/types/asicRegulation';
import { RegulatorPageTemplate } from '~/components/regulators';

// Fetch the FCA regulation data
let regulatorData: AsicRegulationGlobal | null = null;
let error = null;

try {
  regulatorData = await fetchRegulatorGlobal('fca-regulation');
} catch (err) {
  error = err;
  console.error('Error fetching FCA regulation data:', err);
}
---

<RegulatorPageTemplate
  regulatorData={regulatorData}
  error={error}
/>
