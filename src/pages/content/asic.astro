---
import RegulatorCard from "~/components/regulators/RegCardTwo"
import BaseLayout from "~/layouts/BaseLayout.astro"
const response = await fetch('https://api.kenyaforexfirm.com/api/regulators?where[shortName][equals]=asic')

const data = await response.json()
const regulator = data.docs[0]

const {name,
  shortName,
  description,
  country,
  region,
  established,
  logo,
  trustScore,
  slug,} = regulator
  
   const title = 'Best ASIC-regulated Forex Brokers in Kenya',
 // const  description = ''
  
---
<BaseLayout title={title} description ={regulator.description} />
<RegulatorCard
    name={name}
    shortName={shortName}
    description={description}
    country={country}
    region={region}
    established={established}
    logo={logo.url}
    trustScore={trustScore}
    maxLeverage={regulator.maxLeverage}
    slug={slug}
 />
</BaseLayout>

