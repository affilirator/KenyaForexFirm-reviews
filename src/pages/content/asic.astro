---
import RegulatorCard from "~/components/regulators/RegCardTwo.tsx"
import BaseLayout from "~/layouts/BaseLayout.astro"
const response = await fetch('https://api.kenyaforexfirm.com/api/regulators/684e825019528d0a89b10c9d?depth=2&draft=false&locale=en&trash=false')

const regulator = await response.json()

const {name,
  shortName,
  description,
  country,
  region,
  established,
  logo,
  trustScore,
  slug} = regulator;
  
const title = 'Best ASIC-regulated Forex Brokers in Kenya';
 // const  description = ''
  
---
<BaseLayout title={title} description={regulator.description}>
<RegulatorCard
    name={name}
    shortName={shortName}
    description={description}
    country={country}
    region={region}
    established={established}
    logo={logo.url}
    trustScore={trustScore}
    slug={slug}
    client:load
 />
</BaseLayout>

